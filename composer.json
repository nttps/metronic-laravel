{
    "name": "nttps/metronic-theme",
    "description": "Package Laravel Metronic",
    "license": "MIT",
    "authors": [
        {
            "name": "NTTPS",
            "email": "teamerze@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.0"
    },
    "autoload": {
        "psr-4": {
            "Nttps\\MetronicLaravel\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nttps\\MetronicLaravel\\MetronicServiceProvider"
            ],
            "aliases": {
                "Metronic": "Nttps\\MetronicLaravel\\Classes\\Theme\\Metronic",
                "Menu": "Nttps\\MetronicLaravel\\Classes\\\\Theme\\Menu"
            }
        }
    }
}
