{
    "name": "nttps/metronic-theme",
    "description": "Package Laravel Metronic",
    "authors": [
        {
            "name": "NTTPS",
            "email": "teamerze@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.0"
    },
    "autoload": {
        "files": [
            "src/Helpers/helpers.php"
        ],
        "psr-4": {
            "Nttps\\NttpsAuth\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nttps\\MetronicTheme\\NttpsAuthServiceProvider"
            ],
            "aliases": {
                "Metronic": "Nttps\\MetronicTheme\\Theme\\Metronic",
                "Menu": "Nttps\\MetronicTheme\\Theme\\Menu"
            }
        }
    }
}
